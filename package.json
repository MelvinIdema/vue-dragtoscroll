{
  "name": "vue-dragtoscroll",
  "type": "module",
  "version": "1.1.2",
  "description": "A Vue 2 & 3 composition API for drag to scroll.",
  "author": "Melvin Idema",
  "license": "MIT",
  "homepage": "https://github.com/melvinidema/vue-dragtoscroll",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melvinidema/vue-dragtoscroll.git"
  },
  "bugs": {
    "url": "https://github.com/melvinidema/vue-dragtoscroll/issues"
  },
  "keywords": [
    "vue",
    "vue3",
    "composition-api",
    "drag",
    "scroll"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/useDragtoScroll.umd.cjs",
  "module": "dist/useDragtoScroll.js",
  "types": "dist/useDragtoScroll.d.ts",
  "exports": {
    ".": {
      "import": "./dist/useDragtoScroll.js",
      "require": "./dist/useDragtoScroll.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "radix-vue": "^1.9.9",
    "vue": "^3.5.12",
    "vue-demi": "^0.14.10"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.2",
    "vite": "^5.4.10",
    "vitepress": "^1.5.0",
    "vue-tsc": "^2.1.8"
  }
}
